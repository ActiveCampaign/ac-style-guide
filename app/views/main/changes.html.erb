<div class="display-wrap">
	<div class="row">
		<div class="col-xs-12">
			<h5>📝 What's New?</h5>
		</div>
	</div>
		<% @change_days.each do |day, changes| %>
		<div class="change--day">
			<p class="text_size-4"><strong><%= day.strftime("%B %d, %Y") %></strong></p>
			<table class="style1">
			<% changes.each do |change| %>
			
				<tr>
					<td><%= change.description %></td>
				</tr>
			
			<% end %>
			</table>
		</div>
		<% end %>
		
</div>